<%- model_class = User -%>

<div class="col-lg-12 col-md-12">
  <div class="page-header">
    All Users | <%= link_to t('.new', :default => t("helpers.links.new")),
            new_admin_user_path,
            :class => 'btn btn-primary' %>
  </div>  

<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>

    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>